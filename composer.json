{
    "name": "rougin/weasley",
    "description": "Generators and helpers for improving your Slytherin workflow",
    "homepage": "https://github.com/rougin/weasley",
    "license": "MIT",
    "keywords": [
        "helper",
        "generators",
        "slytherin",
        "workflow",
        "weasley"
    ],
    "authors": [
        {
            "name": "Rougin Royce Gutib",
            "email": "rougingutib@gmail.com",
            "homepage": "http://rougin.github.io",
            "role": "Developer"
        }

        "rougin/slytherin": "~0.9",
        "symfony/console": "~2.8 || ~3.0",
        "vlucas/valitron": "~1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "scrutinizer/ocular": "~1.1"

{
    "name": "rougin/weasley",
    "description": "Generators and helpers for the Slytherin framework.",
    "keywords": ["helper", "generator", "slytherin", "workflow", "weasley"],
    "homepage": "https://roug.in/weasley/",
    "license": "MIT",
    "authors":
    [
        {
            "email": "rougingutib@gmail.com",
            "homepage": "https://roug.in/",
            "name": "Rougin Gutib",
            "role": "Software Engineer"
        }
    ],
    "require":
    {
        "php": ">=5.3.0",
        "http-interop/http-middleware": "^0.4.1",
        "rougin/slytherin": "~0.9",
        "symfony/console": "~2.8|~3.0|~4.0|~5.0|~6.0",
        "vlucas/valitron": "~1.4"
    },
    "require-dev":
    {
        "phpunit/phpunit": "~4.2|~5.7|~6.0|~7.0|~8.0|~9.0",
        "sanmai/phpunit-legacy-adapter": "~6.1|~8.0"
    },
    "bin":
    [
        "bin/weasley",
        "bin/weasley.php"
    ],
    "autoload":
    {
        "psr-4":
        {
            "Rougin\\Weasley\\": "src"
        }
    },
    "autoload-dev":
    {
        "psr-4":
        {
            "Rougin\\Weasley\\": "tests"
        }
    }
}